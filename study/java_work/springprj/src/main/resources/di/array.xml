				<entry>
					<key>
						<value>조로</value>
					</key>
					<value>이도류</value>
				</entry>
				<entry>
					<key>
						<value>상어</value>
					</key>
					<value>어류</value>
				</entry>
			</map>
		</constructor-arg>
	</bean>
	
</beans>